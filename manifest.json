{
    "manifest_version": 2,
    "name": "DocApiParser",
    "version": "0.0.1",
    "default_locale": "zh_CN",
    "description": "__MSG_description__",
    "icons": {
        "16": "images/css16.png",
        "32": "images/css32.png",
        "48": "images/css48.png",
        "128": "images/css128.png"
    },
    "page_action": {
        "default_icon": {
            "16": "images/css16.png",
            "32": "images/css32.png",
            "48": "images/css48.png",
            "128": "images/css128.png"
        },
        "default_popup": "popup/popup.html",
        "default_title": "MD -> MOCK API"
    },
    "background": {
        "scripts": ["bg.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "downloads",
        "http://*/"
    ]
}